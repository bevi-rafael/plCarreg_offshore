{
	"_version": "1.5.0",
	"sap.app": {
		"id": "com.eldorado.ewm.plCarregOffshore",
		"type": "application",
		"i18n": "i18n/i18n.properties",
		"applicationVersion": {
			"version": "1.0.0"
		},
		"title": "{{appTitle}}",
		"description": "{{appDescription}}",
		"sourceTemplate": {
			"id": "ui5template.basicSAPUI5ApplicationProject",
			"version": "1.40.12"
		},
        "dataSources": {
            "ZGTW_PLCARREG_OFFSHORE_SRV": {
                "uri": "/sap/opu/odata/sap/ZGTW_PLCARREG_OFFSHORE_SRV/",
                "type": "OData",
                "settings": {
                   "odataVersion": "2.0",
                   "localUri": "localService/ZGTW_PLCARREG_OFFSHORE/metadata.xml"
                }
            }
        }
	},

	"sap.ui": {
		"technology": "UI5",
		"icons": {
			"icon": "",
			"favIcon": "",
			"phone": "",
			"phone@2": "",
			"tablet": "",
			"tablet@2": ""
		},
		"deviceTypes": {
			"desktop": true,
			"tablet": true,
			"phone": true
		},
		"supportedThemes": [
			"sap_hcb",
			"sap_belize"

		],
		"fullWidth": true
	},

	"sap.ui5": {
		"config": {
            "fullWidth": true
        },
        "rootView": {
           "viewName": "com.eldorado.ewm.plCarregOffshore.view.S0_App",
           "type": "XML"
		},
		"dependencies": {
			"minUI5Version": "1.30.0",
			"libs": {
				"sap.ui.core": {},
				"sap.m": {},
				"sap.ui.layout": {},
				"sap.ushell": {},
				"sap.collaboration": {},
				"sap.ui.comp": {},
				"sap.uxap": {}
			}
		},
		"contentDensities": {
			"compact": true,
			"cozy": true
		},
		"models": {
			"i18n": {
				"type": "sap.ui.model.resource.ResourceModel",
				"settings": {
					"bundleName": "com.eldorado.ewm.plCarregOffshore.i18n.i18n"
				}
			},
            "DadosOffshore": {
            	"uri": "/sap/opu/odata/sap/ZGTW_PLCARREG_OFFSHORE_SRV/",
            	"type": "sap.ui.model.odata.v2.ODataModel",
            	"settings": {
					"defaultOperationMode": "Server",
					"defaultBindingMode": "OneWay",
					"defaultCountMode": "None"
            	},
            	"dataSource": "ZGTW_PLCARREG_OFFSHORE_SRV",
            	"preload": true
            },
            "lgnum": {
            	"type": "sap.ui.model.json.JSONModel"
            }
		},
		"resources": {
			"css": [{
				"uri": "css/style.css"
			}]
		},
		"routing": {
			"config": {
				"routerClass": "sap.m.routing.Router",
				"viewPath": "com.eldorado.ewm.plCarregOffshore.view",
				"viewType": "XML",
				"controlId": "appStart",
				"controlAggregation": "pages",
				"clearAggregation": true,
				"transition": "slide",
				"async": true,
				"bypassed": {
					"target": "notFound"
				}
			},
			"routes": [
				{
					"name": "mainScreen",
					"pattern": "",
					"target": [ "s1MainScreen" ]
				}
			],
			"targets": {
				"notFound": {
					"viewId": "notFound",
					"viewName": "NotFound",
					"viewType": "XML",
					"transition": "show"
				},
				"s1MainScreen": {
					"viewId": "s1MainScreen",
					"viewName": "S1_MainScreen",
					"viewType": "XML",
					"viewLevel": 1
				}
			}
		}
	}
}